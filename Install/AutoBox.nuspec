<?xml version="1.0" encoding="utf-8" ?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>AutoBox</id>
    <version>1.1.0</version>
    <authors>Mehfuz Hossain</authors>
    <summary>On the fly dependency Injection and caching container</summary>
    <description>
      The tool dynamically injects dependencies based on convention. Also, let you cache repository calls to memcached (the most poular cross-plaform caching system).
    </description>
    <language>en-US</language>
    <projectUrl>http://mehfuzh.github.com/AutoBox/</projectUrl>
    <licenseUrl>http://www.opensource.org/licenses/mit-license.php</licenseUrl>
    <tags>caching configuration  memcache asp.net mvc windsor</tags>

    <dependencies>
      <dependency id="Castle.Windsor" version="3.0.0.4001" />
      <dependency id="CommonServiceLocator" version="1.0" />
      <dependency id="Newtonsoft.Json" version="4.0.3" />
    </dependencies>
    
  </metadata>
  
  <files>
    
    <file src="..\..\Content\web.config.transform" target="content"/>
    <file src="..\..\bin\Debug\AutoBox.dll" target="lib/Net40"/>
    <file src="..\..\bin\Debug\AutoBox.XML" target="lib/Net40"/>
    <file src="..\..\bin\Debug\BeIT.MemCached.dll" target="lib/Net40"/>
   
  </files>
</package>